{
    "name": "leettech/devnota-php",
    "description": "Permission handling for Laravel 8.0 and up",
    "license": "MIT",
    "keywords": [
        "nfse",
        "laravel",
        "nfse-an"
    ],
    "homepage": "https://github.com/leettech/devnota-php",
    "require": {
        "php": "^8.0",
        "illuminate/container": "^8.12|^9.0|^10.0|^11.0|^12.0",
        "illuminate/contracts": "^8.12|^9.0|^10.0|^11.0|^12.0",
        "illuminate/database": "^8.12|^9.0|^10.0|^11.0|^12.0",
        "illuminate/support": "^8.12|^9.0|^10.0|^11.0|^12.0",
        "spatie/array-to-xml": "^3.4"
    },
    "require-dev": {
        "larastan/larastan": "^3.8",
        "laravel/pint": "^1.0",
        "nunomaduro/larastan": "^3.8",
        "orchestra/testbench": "^6.23|^7.0|^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^9.4|^10.1|^11.5",
        "rector/rector": "^2.2"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "NFSe\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "NFSe\\Tests\\": "tests/",
            "NFSe\\Database\\Factories\\": "database/factories/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {},
        "laravel": {
            "providers": [
                "NFSe\\NFSeServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit",
        "format": "pint",
        "analyse": "echo 'Checking dependencies...' && composer require --dev larastan/larastan && phpstan analyse"
    }
}